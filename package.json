{
  "name": "mrbarry.com",
  "version": "1.0.0",
  "description": "Personal and Professional portfolio",
  "dependencies": {
    "hyperapp": "^2.0.1",
    "hyperapp-debug": "^1.0.0-beta.4",
    "hyperapp-fx": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "firebase-tools": "^7.0.0",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.21.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "start": "parcel ./src/apps/index/index.html",
    "start:test-bed": "parcel ./src/apps/test-bed/index.html",
    "build:index": "parcel build ./src/apps/index/index.html",
    "build:testbed": "parcel build ./src/apps/test-bed/index.html -d ./dist/test-bed/ --public-url /test-bed --no-minify",
    "build": "yarn build:index && yarn build:testbed",
    "deploy": "yarn build && firebase deploy"
